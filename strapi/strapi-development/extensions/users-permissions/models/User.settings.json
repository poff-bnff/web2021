{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "people": {
      "via": "user",
      "collection": "person"
    },
    "profileFilled": {
      "type": "boolean"
    },
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "phoneNr": {
      "type": "biginteger"
    },
    "birthdate": {
      "type": "date"
    },
    "address": {
      "type": "string"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "externalProviders": {
      "type": "component",
      "repeatable": true,
      "component": "user.external-providers"
    },
    "last10Logins": {
      "type": "component",
      "repeatable": true,
      "component": "user.last10-logins",
      "max": 10
    }
  }
}
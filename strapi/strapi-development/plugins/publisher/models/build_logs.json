{
  "kind": "collectionType",
  "collectionName": "build_logs",
  "info": {
    "name": "Deploy log",
    "description": "Logs from pushing the buttons in publisher plugin"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "site": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "error_code": {
      "type": "string"
    },
    "start_time": {
      "type": "datetime",
      "required": false
    },
    "end_time": {
      "type": "datetime",
      "required": false
    },
    "build_errors": {
      "type": "richtext"
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    },
    "queued_time": {
      "type": "datetime"
    },
    "also_builds": {
      "plugin": "publisher",
      "collection": "build_logs",
      "via": "built_by"
    },
    "built_by": {
      "plugin": "publisher",
      "model": "build_logs",
      "via": "also_builds"
    },
    "build_est_duration": {
      "type": "integer"
    },
    "queue_est_duration": {
      "type": "integer"
    },
    "no_estimate_builds_in_queue": {
      "type": "integer"
    },
    "in_queue": {
      "type": "integer"
    },
    "duration": {
      "type": "integer"
    },
    "build_args": {
      "type": "string"
    }
  }
}

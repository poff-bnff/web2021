{
  "kind": "collectionType",
  "collectionName": "build_logs",
  "info": {
    "name": "Build and publish logs"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "site": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "text",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "startTime": {
      "type": "date",
      "required": false
    },
    "endTime": {
      "type": "date",
      "required": false
    }
  }
}




{
  "routes": [
    {
      "method": "POST",
      "path": "/publish",
      "handler": "publisher.publish",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    },
    {
      "method": "GET",
      "path": "/logs/:site",
      "handler": "publisher.logs",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/onelog/:id",
      "handler": "publisher.onelog",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/fullbuild",
      "handler": "publisher.fullBuild",
      "config": {
        "policies": ["plugins::users-permissions.ratelimit"]
      }
    }

  ]
}

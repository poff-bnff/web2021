{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "name": "Product_Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "namePrivate": {
      "type": "string"
    },
    "name": {
      "type": "component",
      "repeatable": false,
      "component": "translated.translated-short"
    },
    "festival_edition": {
      "model": "festival-edition"
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "translated.translated-long"
    },
    "validityPeriod": {
      "type": "component",
      "repeatable": true,
      "component": "time.period"
    },
    "salesPeriod": {
      "type": "component",
      "repeatable": true,
      "component": "time.period"
    },
    "ticketingRight": {
      "type": "component",
      "repeatable": true,
      "component": "ticketing.ticketing-rights"
    },
    "priceAtPeriod": {
      "type": "component",
      "repeatable": true,
      "component": "ticketing.price-with-time"
    },
    "codePrefix": {
      "type": "string",
      "maxLength": 3,
      "minLength": 2
    },
    "slug_et": {
      "type": "uid",
      "targetField": "name_et"
    },
    "slug_en": {
      "type": "uid",
      "targetField": "name_en"
    },
    "slug_ru": {
      "type": "uid",
      "targetField": "name_ru"
    },
    "images": {
      "type": "component",
      "repeatable": false,
      "component": "media.imgs"
    },
    "user_roles": {
      "collection": "user-role",
      "via": "product_categories",
      "dominant": true
    },
    "validity_period_relative": {
      "type": "component",
      "repeatable": false,
      "component": "time.period-relative"
    },
    "name_et": {
      "type": "string"
    },
    "name_en": {
      "type": "string"
    },
    "name_ru": {
      "type": "string"
    },
    "business_profile": {
      "model": "business-profile"
    },
    "courses": {
      "collection": "course",
      "via": "product_categories",
      "dominant": true
    },
    "course_events": {
      "via": "product_categories",
      "collection": "course-event"
    },
    "transferable": {
      "type": "boolean",
      "default": false
    },
    "free": {
      "type": "boolean",
      "default": false
    },
    "product_types": {
      "collection": "product-type"
    },
    "hidden": {
      "type": "boolean"
    }
  }
}

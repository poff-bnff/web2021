{
  "kind": "collectionType",
  "collectionName": "person_tests",
  "info": {
    "name": "PersonTest"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "phoneNr": {
      "type": "biginteger"
    },
    "birthdate": {
      "type": "date"
    },
    "address": {
      "type": "string"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "person-test"
    }
  }
}

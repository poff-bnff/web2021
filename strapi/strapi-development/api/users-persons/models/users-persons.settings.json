{
  "kind": "collectionType",
  "collectionName": "users_persons",
  "info": {
    "name": "UsersPersons"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "users_permissions_user": {
      "via": "users_person",
      "plugin": "users-permissions",
      "model": "user"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phoneNr": {
      "type": "string"
    }
  }
}

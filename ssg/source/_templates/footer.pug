include /_templates/mixins.pug

footer
    .grid_footer_row
        .footer_row_item
            if self.footer.socialGroup
                //- each socialGroup in self.footer.socialGroup
                if self.footer.socialGroup.name
                    p.footer_h= self.footer.socialGroup.name
                if self.footer.socialGroup.items
                    each item in self.footer.socialGroup.items
                        if item.svgCode
                            a(href=item.url target='_blank') !{item.svgCode}
                        else if item.svgMedia
                            img.footer_logo_item(alt=item.name data-src=`https://assets.poff.ee/img/${item.svgMedia.hash}${item.svgMedia.ext}` src=`/https://assets.poff.ee/img/${item.svgMedia.hash}${item.svgMedia.ext}`)


        if self.footer.item
            each item in self.footer.item
                .footer_row_item
                    if item.name && (typeof(item.name) !== 'object')
                        p.footer_h= item.name
                    if item.svgItem
                        each svgItem in item.svgItem
                            if svgItem.svgCode
                                if svgItem.url && (svgItem.url.substr(0, 4).toLowerCase() === 'http')
                                    - var urlToDisplay = svgItem.url
                                else
                                    - var urlToDisplay = `/${lang_path}${svgItem.url}`
                                a(href=urlToDisplay target='_blank') !{svgItem.svgCode}
                            else if svgItem.svgMedia
                                img.footer_logo_item(alt=svgItem.name data-src=`https://assets.poff.ee/img/${svgItem.svgMedia.hash}${svgItem.svgMedia.ext}` src=`https://assets.poff.ee/img/${svgItem.svgMedia.hash}${svgItem.svgMedia.ext}`)
                    if item.image
                        img.footer_logo_item_1(alt=item.name data-src=`https://assets.poff.ee/img/${item.image.hash}${item.image.ext}`  src=`https://assets.poff.ee/img/${item.image.hash}${item.image.ext}`)
                    if item.text
                        .footer_row_item_text
                            if typeof(item.text) !== 'object'
                                +format(item.text).footer_h
                            else
                                p.footer_h
                                    | !{item.text}



    if self.footer.supporters
        - var sortedSupportersCat = self.footer.supporters.sort(function(a, b){ return a.order-b.order })
        section.supporters_footer
            each supporters in sortedSupportersCat
                row
                    if supporters.name && (typeof(supporters.name) !== 'object')
                        h1.section_headline_footer= supporters.name
                    if supporters.supporter
                        .footer_logos
                            each supporter in supporters.supporter
                                if supporter.customUrl
                                    a(href= supporter.customUrl).footer_logo_item_container
                                        if supporter.logoWhite
                                            img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoWhite.hash}${supporter.logoWhite.ext}`  src=`https://assets.poff.ee/img/${supporter.logoWhite.hash}${supporter.logoWhite.ext}`)
                                        else if supporter.logoBlack
                                            img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoBlack.hash}${supporter.logoBlack.ext}`  src=`https://assets.poff.ee/img/${supporter.logoBlack.hash}${supporter.logoBlack.ext}`)
                                        else if supporter.logoColour
                                            img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoColour.hash}${supporter.logoColour.ext}`  src=`https://assets.poff.ee/img/${supporter.logoColour.hash}${supporter.logoColour.ext}`)
                                else
                                    if supporter.logoWhite
                                        img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoWhite.hash}${supporter.logoWhite.ext}`  src=`https://assets.poff.ee/img/${supporter.logoWhite.hash}${supporter.logoWhite.ext}`)
                                    else if supporter.logoBlack
                                        img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoBlack.hash}${supporter.logoBlack.ext}`  src=`https://assets.poff.ee/img/${supporter.logoBlack.hash}${supporter.logoBlack.ext}`)
                                    else if supporter.logoColour
                                        img.footer_logo_item(alt=supporter.url data-src=`https://assets.poff.ee/img/${supporter.logoColour.hash}${supporter.logoColour.ext}`  src=`https://assets.poff.ee/img/${supporter.logoColour.hash}${supporter.logoColour.ext}`)




